{
    "editor.lineHeight": 0,
    "editor.fontFamily": "'FiraCode Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontWeight": 500,
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.shell.linux": "/usr/bin/xonsh",
    "window.menuBarVisibility": "toggle",
    "prettier.arrowParens": "avoid",
    "prettier.printWidth": 100,
    "prettier.trailingComma": "none",
    "workbench.startupEditor": "welcomePage",
    "workbench.activityBar.visible": true,
    "prettier.tabWidth": 4,
    "explorer.confirmDelete": false,
    "liveServer.settings.donotShowInfoMsg": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "explorer.confirmDragAndDrop": false,
    "javascript.preferences.importModuleSpecifierEnding": "js",
    "typescript.preferences.importModuleSpecifierEnding": "js",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "files.exclude": {
        "**/node_modules": true
    },
    "prettier.disableLanguages": [
        "svg"
    ],
    "prettier.documentSelectors": [
        "!svg"
    ],
    "svg.completion.emmet": true,
    "svg.preview.mode": "svg",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.cursorBlinking": "expand",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 2,
    "terminal.external.linuxExec": "konsole",
    "terminal.integrated.enableBell": true,
    "terminal.integrated.rendererType": "canvas",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.colorTheme": "Community Material Theme Ocean",
    "editor.formatOnSave": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.wordWrap": "on",
    "editor.cursorSmoothCaretAnimation": true,
    "cSpell.language": "en,fr",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "material-icon-theme.folders.theme": "specific",
    "workbench.colorCustomizations": {
        "[Community Material Theme Ocean]": {
            "activityBar.activeBorder": "#3f51b5",
            "progressBar.background": "#3f51b5",
            "pickerGroup.foreground": "#3f51b5",
            "textLink.foreground": "#3f51b5",
            "panelTitle.activeBorder": "#3f51b5",
            "editor.findMatchBorder": "#3f51b5",
            "activityBarBadge.background": "#3f51b5",
            "scrollbarSlider.activeBackground": "#3f51b5",
            "tab.activeBorder": "#3f51b5",
            "editorWidget.resizeBorder": "#3f51b5",
            "editorWidget.border": "#3f51b5",
            "statusBarItem.remoteBackground": "#3f51b5",
            "editorOverviewRuler.findMatchForeground": "#3f51b5",
            "list.activeSelectionForeground": "#3f51b5",
            "list.inactiveSelectionForeground": "#3f51b5",
            "list.highlightForeground": "#3f51b5",
            "editorSuggestWidget.highlightForeground": "#3f51b5",
            "notificationLink.foreground": "#3f51b5",
            "breadcrumb.activeSelectionForeground": "#3f51b5",
            "menu.selectionForeground": "#3f51b5",
            "menubar.selectionForeground": "#3f51b5",
            "settings.headerForeground": "#3f51b5",
            "settings.modifiedItemIndicator": "#3f51b5",
            // Terminal
            // "terminal.background": "#080808",
            // "terminal.border": "",
            // "terminal.foreground": "",
            "terminal.ansiBlack": "#080808",
            "terminal.ansiBrightBlack": "#080808",
            "terminal.ansiBrightRed": "#B53F3F",
            "terminal.ansiRed": "#B53F3F",
            "terminal.ansiBrightGreen": "#48B53F",
            "terminal.ansiGreen": "#48B53F",
            "terminal.ansiBrightYellow": "#B5B13F",
            "terminal.ansiYellow": "#B5B13F",
            "terminal.ansiBlue": "#3F51B5",
            "terminal.ansiBrightBlue": "#3F51B5",
            "terminal.ansiBrightMagenta": "#8A3FB5",
            "terminal.ansiMagenta": "#8A3FB5",
            "terminal.ansiBrightCyan": "#3FB5A7",
            "terminal.ansiCyan": "#3FB5A7",
            "terminal.ansiBrightWhite": "#ffffff",
            "terminal.ansiWhite": "#ffffff",
            // "terminal.selectionBackground": "",
            // "terminalCursor.background": "",
            // "terminalCursor.foreground": "",
        }
    },
    "window.zoomLevel": 0,
}